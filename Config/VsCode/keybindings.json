	//s15-------------------
	// ====Vim.remap==== //
	{
		"key": "shift+down", // quick insert mode
		"command": "vim.remap",
		"args": {
			"after": [
				"i"
			]
		},
		"when": "editorTextFocus && vim.mode != 'Insert'",
	},
	{
		"key": "shift+up", // quick visual mode
		"command": "vim.remap",
		"args": {
			"after": [
				"v"
			]
		},
		"when": "editorTextFocus&& vim.mode != 'Insert'",
	},
	{
		"key": "ctrl+]", // move focus to right
		"command": "vim.remap",
		"args": {
			"after": [
				"<C-w>",
				"l"
			]
		},
		"when": "vim.mode != 'Insert'",
	},
	{
		"key": "ctrl+[", // move focus to left
		"command": "vim.remap",
		"args": {
			"after": [
				"<C-w>",
				"h"
			]
		},
		"when": "vim.mode != 'Insert'",
	},
