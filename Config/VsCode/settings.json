{
  // --font size
  "editor.fontSize": 19.5,
  "debug.console.fontSize": 14,
  // --Font
  "editor.fontLigatures": false,
  "editor.fontFamily": "Anonymous Pro, Hack Nerd Font, monospace",
  // --tab key
  "editor.tabCompletion": "on",
  "editor.tabSize": 2,
  // --code runner
  "code-runner.saveFileBeforeRun": true,
  "code-runner.clearPreviousOutput": true,
  "code-runner.executorMapByGlob": {
    "code-runner.executorMapByGlob": {}
  },
  // --live server
  "liveServer.settings.donotShowInfoMsg": true,
  "liveServer.settings.donotVerifyTags": true,
  // --Terminal
  "terminal.integrated.fontSize": 25,
  "terminal.integrated.tabs.defaultIcon": "home",
  "terminal.explorerKind": "external",
  "terminal.integrated.tabs.title": "",
  "terminal.integrated.env.osx": {
    "FIG_NEW_SESSION": "1"
  },
  "terminal.integrated.shellIntegration.suggestEnabled": true,
  // --debug
  "debug.terminal.clearBeforeReusing": true,
  // --Always keep the cursor in the center of the screen
  "editor.cursorSurroundingLines": 100,
  // --explorer
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  // --icon
  "workbench.iconTheme": "material-icon-theme",
  // --auto save
  "files.autoSave": "onFocusChange",
  // --Copy | vim
  "vim.useSystemClipboard": true,
  "vim.overrideCopy": true,
  // "vim.sneakUseIgnorecaseAndSmartcase": true,
  "vim.hlsearch": true,
  "vim.visualstar": true,
  "vim.startInInsertMode": false,
  // --Cursor | vim
  "vim.cursorStylePerMode.insert": "line",
  "vim.cursorStylePerMode.normal": "block",
  "vim.cursorStylePerMode.visual": "block",
  "vim.cursorStylePerMode.replace": "underline",
  // --status bar | vim
  "vim.statusBarColorControl": true,
  "vim.statusBarColors.visual": "#0E4E4E",
  "vim.statusBarColors.normal": "#1F1B2C",
  "vim.statusBarColors.replace": "#420A7F",
  "vim.statusBarColors.insert": "#500128",
  "vim.statusBarColors.commandlineinprogress": [
    "#5c6300"
  ],
  "vim.incsearch": true,
  "vim.easymotionDimBackground": false,
  "vim.targets.smartQuotes.breakThroughLines": false,
  "vim.targets.smartQuotes.aIncludesSurroundingSpaces": false,
  "vim.vimrc.path": "$HOME/.config/vscode/.vscodevimrc",
  "vim.vimrc.enable": true,
  "vim.highlightedyank.enable": true,
  "vim.leader": "<space>",
  "vim.smartRelativeLine": true,
  "vim.foldfix": false,
  "vim.startofline": false,
  "whichkey.bindingOverrides": [
    // {
    //   "keys": "t.s",
    //   "name": "Go to line",
    //   "type": "command",
    //   "command":"workbench.action.gotoLine",
    // },
  ],
  // --VisualVim
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": [
        "leader",
        "s"
      ],
      "commands": [
        "workbench.action.findInFiles"
      ]
    },
    // {
    //   "before": [
    //     "<space>"
    //   ],
    //   "commands": [
    //     "vspacecode.space"
    //   ]
    // },
    {
      "before": [
        ">"
      ],
      "commands": [
        "editor.action.indentLines"
      ]
    },
    {
      "before": [
        "<"
      ],
      "commands": [
        "editor.action.outdentLines"
      ]
    }
  ],
  // --NormalVim
  "vim.normalModeKeyBindings": [
    // {
    //   "before": [
    //     "leader",
    //     "leader"
    //   ],
    //   "commands": [
    //     "whichkey.show"
    //   ]
    // },
    {
      "before": [
        "leader",
        "c"
      ],
      "commands": [
        "whichkey.show"
      ]
    },
    //Explorer
    {
      "before": [
        "leader",
        "n"
      ],
      "commands": [
        "workbench.view.explorer"
      ]
    },
    // open new terminal-external
    {
      "before": [
        "leader",
        "leader",
        "leader",
        "'"
      ],
      "commands": [
        "workbench.action.files.save"
        // "workbench.action.terminal.openNativeConsole"
      ]
    },
    {
      "before": [
        "leader",
        "leader",
        "leader",
        "l"
      ],
      "commands": [
        "workbench.action.gotoLine"
      ]
    },
    // close others tab
    {
      "before": [
        "leader",
        "leader",
        "leader",
        "w",
        "q"
      ],
      "commands": [
        "workbench.action.closeAllEditors"
      ]
    },
    // minimap
    {
      "before": [
        "leader",
        "m"
      ],
      "commands": [
        "editor.action.toggleMinimap"
      ]
    },
    // move focus to statusbar
    {
      "before": [
        "leader",
        "b"
      ],
      "commands": [
        "workbench.action.focusStatusBar"
      ]
    },
    // remove all Breakpoints
    {
      "before": [
        "leader",
        "a",
        "d"
      ],
      "commands": [
        "workbench.debug.viewlet.action.removeAllBreakpoints"
      ]
    },
    // toggle Breakpoints
    {
      "before": [
        "leader",
        "d"
      ],
      "commands": [
        "editor.debug.action.toggleBreakpoint"
      ]
    },
    // start debug
    {
      "before": [
        "leader",
        "leader",
        "d"
      ],
      "commands": [
        "workbench.action.debug.start"
      ]
    },
    // problems
    {
      "before": [
        "leader",
        "p"
      ],
      "commands": [
        "workbench.actions.view.problems"
      ]
    },
    // {
    //   "before": [
    //     "leader",
    //     "leader",
    //     "leader",
    //     "p"
    //   ],
    //   "after": [
    //     "v"
    //   ],
    //   "commands": [
    //     "editor.action.marker.nextInFiles"
    //   ],
    // },
    //
    // source control
    {
      "before": [
        "leader",
        "g"
      ],
      "commands": [
        "workbench.view.scm"
      ]
    },
    // search in project
    {
      "before": [
        "leader",
        "leader",
        "s"
      ],
      "commands": [
        "workbench.action.experimental.quickTextSearch"
      ]
    },
    // fold
    {
      "before": [
        "leader",
        "leader",
        "leader",
        "["
      ],
      "commands": [
        "editor.fold"
      ]
    },
    // unfold
    {
      "before": [
        "leader",
        "leader",
        "leader",
        "]"
      ],
      "commands": [
        "editor.unfold"
      ]
    },
    // switch windows
    {
      "before": [
        "leader",
        "up"
      ],
      "commands": [
        "editor.action.focusStickyScroll"
      ]
    },
    // jump to function
    {
      "before": [
        "leader",
        "f"
      ],
      "commands": [
        "extension.vim_ctrl+]"
      ]
    },
    // recent
    {
      "before": [
        "leader",
        "z"
      ],
      "commands": [
        "workbench.action.openRecent"
      ]
    },
    // timeline
    {
      "before": [
        "leader",
        "l"
      ],
      "commands": [
        "timeline.focus"
      ]
    },
    // quit all
    {
      "before": [
        "leader",
        "q"
      ],
      "commands": [
        "workbench.action.closeSidebar",
        "workbench.action.closeAuxiliaryBar",
        "workbench.action.closePanel"
      ]
    },
    // opened recently tab
    {
      "before": [
        "leader",
        "/"
      ],
      "commands": [
        "workbench.action.quickOpenPreviousRecentlyUsedEditorInGroup"
      ]
    },
    // terminal
    {
      "before": [
        "leader",
        "t"
      ],
      "commands": [
        "workbench.action.terminal.focus"
      ]
    },
    // file browse
    {
      "before": [
        "leader",
        "leader",
        "n"
      ],
      "commands": [
        "file-browser.open"
      ]
    },
    // Go to bracket "{}[]()"
    {
      "before": [
        "'"
      ],
      "commands": [
        "editor.action.jumpToBracket"
      ]
    },
    {
      "before": [
        "leader",
        "leader",
        "leader",
        "."
      ],
      "commands": [
        "editor.action.showDefinitionPreviewHover"
      ]
    },
    // select text
    {
      "before": [
        "leader",
        "S"
      ],
      "after": [
        "B",
        "v",
        "E"
        // "y"
      ]
    },
    {
      "before": [
        "?"
      ],
      "commands": [
        "fuzzySearch.activeTextEditor"
      ]
    },
    {
      "before": [
        "leader",
        "o"
      ],
      "commands": [
        "editor.action.openLink"
      ]
    },
    {
      "before": [
        "f"
      ],
      "commands": [
        "breadcrumbs.focusAndSelect"
      ]
    },
    {
      "before": [
        "\\",
        "h"
      ],
      "commands": [
        "workbench.action.moveEditorToLeftGroup"
      ]
    },
    {
      "before": [
        "\\",
        "l"
      ],
      "commands": [
        "workbench.action.moveEditorToRightGroup"
      ]
    },
    {
      "before": [
        "\\",
        "j"
      ],
      "commands": [
        "workbench.action.moveEditorToBelowGroup"
      ]
    },
    {
      "before": [
        "\\",
        "k"
      ],
      "commands": [
        "workbench.action.moveEditorToAboveGroup"
      ]
    }
  ],
  "workbench.colorCustomizations": {
    "[Palenight Italic]": {
      "editorCursor.foreground": "#ffffff"
    },
    "[Omni]": {
      "editorBracketHighlight.foreground1": "#fff86a",
      "editorBracketHighlight.foreground2": "#ffaa00",
      "editorBracketHighlight.foreground3": "#ff62bb",
      "editorBracketHighlight.foreground4": "#9395ff",
      "editorBracketHighlight.foreground5": "#76fffd",
      "editorBracketHighlight.foreground6": "#00eb5e",
      "editorIndentGuide.background1": "#ffffff31",
      "editorGutter.addedBackground": "#ffffffdb",
      "editorGutter.deletedBackground": "#ff4848",
      "editorGutter.commentRangeForeground": "#00ffee",
      "editorGutter.commentUnresolvedGlyphForeground": "#00ffe1",
      "editorGutter.commentGlyphForeground": "#00ff2a",
      "editorGutter.foldingControlForeground": "#ff00fb",
      "editorGutter.modifiedBackground": "#00ffe5",
      "peekViewTitleDescription.foreground": "#ffffff",
      "peekViewTitleLabel.foreground": "#a6ff00",
      "editorWidget.foreground": "#ffffffe5",
      "editorSuggestWidgetStatus.foreground": "#fbfbfb"
    },
    "statusBar.background": "#1F1B2C",
    "statusBar.noFolderBackground": "#1F1B2C",
    "statusBar.debuggingBackground": "#1F1B2C",
    "statusBar.foreground": "#ffffff",
    "statusBar.debuggingForeground": "#ffffff"
  },
  //e-----------------------------------------
  //s--------------------------------
  // =====UI===== //
  "workbench.colorTheme": "Omni",
  "editor.tokenColorCustomizations": {
    "[Omni]": {
      "comments": "#b5bdf8cd",
      "textMateRules": [
        { // c++
          "scope": "punctuation.definition.directive.cpp",
          "settings": {
            "foreground": "#C792EA"
          }
        },
        { // c++
          "scope": "entity.name.function.operator.member.cpp",
          "settings": {
            "fontStyle": ""
          }
        },
        { // c++
          "scope": "entity.name.function.templated.cpp",
          "settings": {
            "fontStyle": ""
          }
        },
        { // c++
          "scope": "entity.name.function.operator.cpp",
          "settings": {
            "fontStyle": ""
          }
        },
        { // shell
          "scope": "variable.parameter.positional.shell",
          "settings": {
            "foreground": "#00fffb"
          }
        },
        { // shell
          "scope": "entity.name.command.shell",
          "settings": {
            "fontStyle": "underline"
          }
        },
        {
          "scope": "support.type.property-name",
          "settings": {
            "foreground": "#d5bdf8"
          }
        },
        {
          "scope": "constant",
          "settings": {
            "foreground": "#f37e0a"
          }
        },
        { // function
          "scope": "storage",
          "settings": {
            "foreground": "#C792EA"
          }
        },
        { // function
          "scope": "storage.modifier",
          "settings": {
            "foreground": "#C792EA",
            "fontStyle": "italic"
          }
        },
        { // function
          "scope": "support.function",
          "settings": {
            "foreground": "#89DDFF",
            "fontStyle": "italic"
          }
        },
        { // function name
          "scope": "entity.name.function",
          "settings": {
            "foreground": "#82AAFF",
            "fontStyle": "underline"
          }
        },
        { // variable
          "scope": "variable",
          "settings": {
            "foreground": "#FFCB6B"
          }
        },
        { // variable for shell 
          "scope": "variable.other.normal.shell",
          "settings": {
            "foreground": "#FFCB6B"
          }
        },
        { // variable for shell 
          "scope": "variable.other.assignment.shell",
          "settings": {
            "foreground": "#FFCB6B"
          }
        },
        { // =
          "scope": "keyword.operator.assignment",
          "settings": {
            "foreground": "#C792EA",
            "fontStyle": "italic bold"
          }
        },
        { // new
          "scope": "keyword.operator.new.cpp",
          "settings": {
            "fontStyle": "italic bold"
          }
        },
        { // string
          "scope": "string.unquoted",
          "settings": {
            "foreground": "#dfe4ec"
          }
        },
        { // ""
          "scope": "string.quoted",
          "settings": {
            "foreground": "#C3E88D"
          }
        },
        { // "
          "scope": "punctuation.definition.string.end",
          "settings": {
            "foreground": "#D9F5DD"
          }
        },
        { // "
          "scope": "punctuation.definition.string.begin",
          "settings": {
            "foreground": "#D9F5DD"
          }
        },
        { // ""
          "scope": "punctuation",
          "settings": {
            "foreground": "#C3E88D"
          }
        },
        {
          "scope": "constant.character.escape",
          "settings": {
            "foreground": "#F78C6C"
          }
        },
        {
          "scope": "variable.parameter",
          "settings": {
            "foreground": "#7986E7"
          }
        },
        {
          "scope": "keyword.operator",
          "settings": {
            "foreground": "#89DDFF"
          }
        },
        {
          "scope": "constant.numeric",
          "settings": {
            "foreground": "#F78C6C"
          }
        },
        { // if , for 
          "scope": "keyword.control",
          "settings": {
            "foreground": "#C792EA",
            "fontStyle": "italic"
          }
        },
        { // comment
          "scope": "comment",
          "settings": {
            "foreground": "#b5bdf8c9",
            "fontStyle": "italic"
          }
        },
        {
          "scope": "punctuation.support.type.property-name.end,punctuation.support.type.property-name.begin",
          "settings": {
            "foreground": "#e792ffd4"
          }
        },
        { // pointer
          "scope": "storage.modifier.pointer.cpp",
          "settings": {
            "foreground": "#89DDFF"
          }
        },
        {
          "scope": "string.quoted.other.lt-gt.include.cpp",
          "settings": {
            "fontStyle": "italic bold"
          }
        },
        {
          "scope": "storage.type.namespace.directive.cpp",
          "settings": {
            "fontStyle": "italic"
          }
        },
        {
          "scope": "entity.name.command.shell",
          "settings": {
            "fontStyle": ""
          }
        }
      ],
    },
    //start
    "[Monokai]": {
      "comments": "#b5bdf8f1",
      "types": "#77dbfd",
      "variables": "#ceadff",
      "textMateRules": [
        {
          "scope": "comment",
          "settings": {
            "foreground": "#b5bdf8f1",
            "fontStyle": "italic"
          }
        }
      ]
    },
    //start
    "[Dracula]": {
      "textMateRules": [
        {
          "scope": "string",
          "settings": {
            "foreground": "#FFE887"
          }
        },
        {
          "scope": "comment",
          "settings": {
            "foreground": "#b5bdf8f1",
            "fontStyle": "italic"
          }
        }
      ]
    },
    //start
  },
  //e---------------------------------------------------
  "whichkey.bindings": [
    //menu-1
    {
      "key": " ",
      "name": "»»»Search keybindings«««",
      "type": "command",
      "command": "whichkey.searchBindings"
    },
    {
      "key": "\t",
      "name": "Last editor",
      "type": "commands",
      "commands": [
        "workbench.action.navigateToLastEditLocation"
        // "workbench.action.quickOpenPreviousRecentlyUsedEditorInGroup",
        // "list.select"
      ]
    },
    {
      "key": "F",
      "name": "fzf search",
      "icon": "search",
      "command": "extension.multiCommand.execute",
      "args": {
        "sequence": [
          {
            "command": "command-runner.run",
            "args": {
              "command": "fzf-p | xargs -r code"
            }
          },
          {
            "command": "workbench.action.terminal.moveToEditor"
          }
        ]
      }
    },
    {
      "key": "/",
      "name": "Search in project",
      "icon": "search",
      "type": "command",
      "command": "search.action.openNewEditorToSide"
    },
    {
      "key": ".",
      "name": "Repeat most recent action",
      "type": "command",
      "command": "whichkey.repeatMostRecent"
    },
    {
      "key": "b",
      "name": "« Buffers/Editors »",
      "type": "bindings",
      "bindings": [
        {
          "key": "H",
          "name": "Move editor into left group",
          "icon": "triangle-left",
          "type": "command",
          "command": "workbench.action.moveEditorToLeftGroup"
        },
        {
          "key": "L",
          "name": "Move editor into right group",
          "icon": "triangle-right",
          "type": "command",
          "command": "workbench.action.moveEditorToRightGroup"
        },
        {
          "key": "J",
          "name": "Move editor into below group",
          "icon": "triangle-down",
          "type": "command",
          "command": "workbench.action.moveEditorToBelowGroup"
        },
        {
          "key": "K",
          "name": "Move editor into above group",
          "icon": "triangle-up",
          "type": "command",
          "command": "workbench.action.moveEditorToAboveGroup"
        },
        {
          "key": "M",
          "name": "Close other editors",
          "type": "command",
          "command": "workbench.action.closeOtherEditors"
        },
        {
          "key": "P",
          "name": "Paste clipboard to buffer",
          "type": "commands",
          "commands": [
            "editor.action.selectAll",
            "editor.action.clipboardPasteAction"
          ]
        },
        {
          "key": "Y",
          "name": "Copy buffer to clipboard",
          "type": "commands",
          "commands": [
            "editor.action.selectAll",
            "editor.action.clipboardCopyAction",
            "cancelSelection"
          ]
        }
      ]
    },
    {
      "key": "f",
      "name": "« File »",
      "icon": "file",
      "type": "bindings",
      "bindings": [
        {
          "key": "f",
          "name": "Create folding range from selection",
          "type": "command",
          "command": "editor.createFoldingRangeFromSelection"
        },
        {
          "key": "F",
          "name": "Remove manual folding ranges",
          "type": "command",
          "command": "editor.removeManualFoldingRanges"
        },
        {
          "key": "w",
          "name": "Open active in new window",
          "type": "command",
          "command": "workbench.action.files.showOpenedFileInNewWindow"
        },
        {
          "key": "y",
          "name": "Copy path of active file",
          "type": "command",
          "command": "workbench.action.files.copyPathOfActiveFile"
        },
        {
          "key": "c",
          "name": "Change file language",
          "type": "command",
          "command": "workbench.action.editor.changeLanguageMode"
        },
        {
          "key": "=",
          "name": "Format file",
          "type": "command",
          "command": "editor.action.formatDocument"
        }
      ]
    },
    {
      "key": "i",
      "name": "« Insert »",
      "type": "bindings",
      "bindings": [
        {
          "key": "s",
          "name": "Insert snippet",
          "type": "command",
          "command": "editor.action.insertSnippet"
        }
      ]
    },
    {
      "key": "s",
      "name": "Search with selection",
      "icon": "search",
      "type": "bindings",
      "commands": [
        "editor.action.addSelectionToNextFindMatch",
        "workbench.action.findInFiles"
      ]
    },
    {
      "key": "S",
      "name": "« Show »",
      "type": "bindings",
      "bindings": [
        {
          "key": "s",
          "name": "Show search",
          "type": "command",
          "command": "workbench.view.search"
        },
        {
          "key": "g",
          "name": "Show source control",
          "type": "command",
          "command": "workbench.view.scm"
        },
        {
          "key": "t",
          "name": "Show test",
          "type": "command",
          "command": "workbench.view.extension.test"
        },
        {
          "key": "T",
          "name": "Show external terminal",
          "type": "command",
          "command": "workbench.action.terminal.openNativeConsole"
        },
        {
          "key": "r",
          "name": "Show remote explorer",
          "type": "command",
          "command": "workbench.view.remote"
        },
        {
          "key": "e",
          "name": "Show extensions",
          "type": "command",
          "command": "workbench.view.extensions"
        },
        {
          "key": "p",
          "name": "Show problem",
          "type": "command",
          "command": "workbench.actions.view.problems"
        },
        {
          "key": "o",
          "name": "Show output",
          "type": "command",
          "command": "workbench.action.output.toggleOutput"
        },
        {
          "key": "d",
          "name": "Show debug console",
          "type": "command",
          "command": "workbench.debug.action.toggleRepl"
        },
        {
          "key": "\t",
          "name": "« More ... »",
          "type": "command",
          "command": "workbench.action.openView"
        }
      ]
    },
    {
      "key": "u",
      "name": "« UI »",
      "type": "bindings",
      "bindings": [
        {
          "key": "S",
