{
  // font size
  "editor.fontSize": 19.5,
  "terminal.integrated.fontSize": 20,
  "debug.console.fontSize": 14,
  // Font
  "editor.fontLigatures": false,
  "editor.fontFamily": "Anonymous Pro, Hack Nerd Font, monospace",
  // "editor.fontFamily": "Menlo, Monaco, 'Courier New', monospace, Hack Nerd Font",
  //tab key
  "editor.tabCompletion": "on",
  "editor.tabSize": 2,
  // code runner
  "code-runner.saveFileBeforeRun": true,
  "code-runner.clearPreviousOutput": true,
  "code-runner.runInTerminal": true,
  "code-runner.executorMapByGlob": {
    "pom.xml": "cd $dir && mvn clean package"
  },
  // live server
  "liveServer.settings.donotShowInfoMsg": true,
  "liveServer.settings.donotVerifyTags": true,
  // Always keep the cursor in the center of the screen
  "editor.cursorSurroundingLines": 100,
  // 
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  // icon 
  "workbench.iconTheme": "material-icon-theme",
  // auto save
  "files.autoSave": "onFocusChange",
  "vim.incsearch": true,
  // Copy
  "vim.useSystemClipboard": true,
  "vim.overrideCopy": true,
  // "vim.sneakUseIgnorecaseAndSmartcase": true,
  "vim.hlsearch": true,
  "vim.visualstar": true,
  "vim.startInInsertMode": false,
  // Cursor
  "vim.cursorStylePerMode.insert": "line",
  "vim.cursorStylePerMode.normal": "block",
  "vim.cursorStylePerMode.visual": "block",
  // Status Bar color
  "vim.statusBarColorControl": true,
  "vim.statusBarColors.visual": "#0E4E4E",
  "vim.statusBarColors.normal": "#1F1B2C",
  "vim.statusBarColors.replace": "#420A7F",
  "vim.statusBarColors.insert": "#500128",
  "vim.statusBarColors.commandlineinprogress": [
    "#eaff003f",
    "#ff0000"
  ],
  "vim.easymotionDimBackground": false,
  "vim.smartRelativeLine": true,
  "vim.targets.smartQuotes.breakThroughLines": false,
  "vim.targets.smartQuotes.enable": false,
  "vim.targets.smartQuotes.aIncludesSurroundingSpaces": false,
  "vim.startofline": true,
  "vim.autoindent": true,
  "whichkey.bindingOverrides": [],
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": [
        "<space>"
      ],
      "commands": [
        "whichkey.show"
      ]
    }
  ],
  //Start
  "vim.normalModeKeyBindings": [
    {
      "before": [
        "leader",
        "c"
      ],
      "commands": [
        "whichkey.show"
      ]
    },
    //Explorer
    {
      "before": [
        "leader",
        "n"
      ],
      "commands": [
        "workbench.view.explorer"
      ]
    },
    // open new terminal-external
    {
      "before": [
        "leader",
        "leader",
        "leader",
        "'"
      ],
      "commands": [
        "workbench.action.terminal.openNativeConsole"
      ]
    },
    // close others tab 
    {
      "before": [
        "leader",
        "leader",
        "leader",
        "w",
        "q"
      ],
      "commands": [
        "workbench.action.closeAllEditors"
      ]
    },
    // minimap
    {
      "before": [
        "leader",
        "m"
      ],
      "commands": [
        "editor.action.toggleMinimap"
      ]
    },
    // move focus to statusbar
    {
      "before": [
        "leader",
        "b"
      ],
      "commands": [
        "workbench.action.focusStatusBar"
      ]
    },
    // remove all Breakpoints 
    {
      "before": [
        "leader",
        "a",
        "d"
      ],
      "commands": [
        "workbench.debug.viewlet.action.removeAllBreakpoints"
      ]
    },
    // toggle Breakpoints
    {
      "before": [
        "leader",
        "d"
      ],
      "commands": [
        "editor.debug.action.toggleBreakpoint"
      ],
    },
    // start debug
    {
      "before": [
        "leader",
        "leader",
        "d"
      ],
      "commands": [
        "workbench.action.debug.start",
      ]
    },
    // problems
    {
      "before": [
        "leader",
        "p"
      ],
      "commands": [
        "workbench.actions.view.problems"
      ]
    },
    // {
    //   "before": [
    //     "leader",
    //     "leader",
    //     "leader",
    //     "p"
    //   ],
    //   "after": [
    //     "v"
    //   ],
    //   "commands": [
    //     "editor.action.marker.nextInFiles"
    //   ],
    // },
    // 
    // source control
    {
      "before": [
        "leader",
        "g"
      ],
      "commands": [
        "workbench.view.scm"
      ],
    },
    // breadcrumb
    {
      "before": [
        "leader",
        "v"
      ],
      "commands": [
        "workbench.action.gotoSymbol"
      ], // "breadcrumbs.focusAndSelect"
    },
    // search in project
    {
      "before": [
        "leader",
        "leader",
        "s"
      ],
      "commands": [
        "workbench.action.experimental.quickTextSearch"
      ]
    },
    // fold 
    {
      "before": [
        "leader",
        "leader",
        "leader",
        "["
      ],
      "commands": [
        "editor.fold"
      ],
    },
    // unfold
    {
      "before": [
        "leader",
        "leader",
        "leader",
        "]"
      ],
      "commands": [
        "editor.unfold"
      ],
    },
    // switch windows
    {
      "before": [
        "leader",
        "x"
      ],
      "commands": [
        "workbench.action.quickSwitchWindow"
      ]
    },
    // jump to function
    {
      "before": [
        "leader",
        "f"
      ],
      "commands": [
        "extension.vim_ctrl+]"
      ],
    },
    // recent
    {
      "before": [
        "leader",
        "z"
      ],
      "commands": [
        "workbench.action.openRecent"
      ]
    },
    // timeline
    {
      "before": [
        "leader",
        "l"
      ],
      "commands": [
        "timeline.focus"
      ]
    },
    // quit all
    {
      "before": [
        "leader",
        "q"
      ],
      "commands": [
        "workbench.action.closeSidebar",
        "workbench.action.closeAuxiliaryBar",
        "workbench.action.closePanel"
      ]
    },
    // opened recently tab
    {
      "before": [
        "leader",
        "/"
      ],
      "commands": [
        "workbench.action.quickOpenPreviousRecentlyUsedEditorInGroup"
      ],
    },
    // terminal
    {
      "before": [
        "leader",
        "t"
      ],
      "commands": [
        "workbench.action.terminal.focus"
      ],
    },
  ],
