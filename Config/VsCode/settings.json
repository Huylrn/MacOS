  "editor.tokenColorCustomizations": {
    //start 
    "[Omni]": {
      "strings": "#f50202",
      "comments": "#b5bdf8f1",
      // "functions": "#FF0000",
      "types": "#77dbfd",
      // "numbers": "#FF0000",
      // "keywords": "#FF0000",
      "variables": "#ceadff",
      "textMateRules": [
        {
          "scope": "keyword",
          "settings": {
            "foreground": "#fe7abe",
          }
        },
        {
          "scope": "entity.name.function.operator.cpp",
          "settings": {
            "foreground": "#ffffff"
          }
        },
        {
          "scope": "variable.other.local.cpp",
          "settings": {
            "foreground": "#ffffff"
          }
        },
        {
          "scope": "storage",
          "settings": {
            "foreground": "#eaade0",
          }
        },
        {
          "scope": "entity.name.function.operator.member.cpp",
          "settings": {
            "foreground": "#ffffff"
          }
        },
        {
          "scope": "string",
          "settings": {
            "foreground": "#FFE887"
          }
        },
        {
          "scope": "punctuation.support.type.property-name.end,punctuation.support.type.property-name.begin",
          "settings": {
            "foreground": "#e792ffd4"
          }
        },
        {
          "scope": "comment",
          "settings": {
            "foreground": "#b5bdf8f1",
            "fontStyle": "italic"
          }
        }
      ]
    },
    //start
    "[Dracula]": {
      "textMateRules": [
        {
          "scope": "string",
          "settings": {
            "foreground": "#FFE887"
          }
        },
        {
          "scope": "comment",
          "settings": {
            "foreground": "#b5bdf8f1",
            "fontStyle": "italic"
          }
        }
      ]
    },
    //start
    "[Transparent Color Theme]": {
      // "comments": "#ffe98782",
      // "keywords": "#fb8787",
      // "variables": "#7295edca",
      "types": "#FF0000",
      // "numbers": "#FF0000",
      // "functions": "#f75b5b",
      "textMateRules": [
        {
          "scope": "entity.name.tag",
          "settings": {
            "foreground": "#ffaded"
          }
        },
        {
          "scope": "punctuation.definition.comment",
          "settings": {
            "foreground": "#ff56d2",
            "fontStyle": "bold"
          }
        },
        // Hello every
        // OK olala
        {
          "scope": "comment",
          "settings": {
            "foreground": "#daa5fad0",
            "fontStyle": "italic"
          }
        },
        {
          "scope": "string",
          "settings": {
            "foreground": "#baeffb"
          }
        },
        {
          "scope": "entity.other.attribute-name",
          "settings": {
            "foreground": "#fcf589",
            "fontStyle": "italic"
          }
        },
        {
          "scope": "punctuation.definition.tag.end.html, punctuation.definition.tag.begin.html",
          "settings": {
            "foreground": "#ffffffe0"
          }
        },
        // <p>Text<p>
        {
          "scope": "text.html.derivative",
          "settings": {
            "foreground": "#ffffffd5"
          }
        },
        {
          "scope": "punctuation",
          "settings": {
            "foreground": "#ffffff"
          }
        },
        {
          "scope": "variable",
          "settings": {
            "foreground": "#86ff76"
          }
        },
        {
          "scope": "entity.name",
          "settings": {
            "foreground": "#5ba5ff"
          }
        },
        {
          "scope": "constant.numeric",
          "settings": {
            "foreground": "#ffc886"
          }
        },
        {
          "scope": "entity.other.attribute-name.class",
          "settings": {
            "foreground": "#ffa622"
          }
        }
      ]
    }
  },
