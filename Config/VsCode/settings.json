{
	// --font size
	"editor.fontSize": 19.5,
	"debug.console.fontSize": 14,
	// --Font
	"editor.fontLigatures": false,
	"editor.fontFamily": "Anonymous Pro, Hack Nerd Font, monospace",
	// --tab key
	"editor.tabCompletion": "on",
	"editor.tabSize": 2,
	// --code runner
	"code-runner.saveFileBeforeRun": true,
	"code-runner.clearPreviousOutput": true,
	"code-runner.executorMapByGlob": {
		"code-runner.executorMapByGlob": {},
	},
	// --live server
	"liveServer.settings.donotShowInfoMsg": true,
	"liveServer.settings.donotVerifyTags": true,
	// --Terminal
	"terminal.integrated.fontSize": 25,
	"terminal.integrated.tabs.defaultIcon": "home",
	"terminal.explorerKind": "external",
	"terminal.integrated.tabs.title": "",
	"terminal.integrated.env.osx": {
		"FIG_NEW_SESSION": "1"
	},
	"terminal.integrated.shellIntegration.suggestEnabled": true,
	// --debug
	"debug.terminal.clearBeforeReusing": true,
	// --Always keep the cursor in the center of the screen
	"editor.cursorSurroundingLines": 100,
	// --explorer
	"explorer.confirmDelete": false,
	"explorer.confirmDragAndDrop": false,
	// --icon 
	"workbench.iconTheme": "material-icon-theme",
	// --auto save
	"files.autoSave": "onFocusChange",
	// --Copy | vim
	"vim.useSystemClipboard": true,
	"vim.overrideCopy": true,
	// "vim.sneakUseIgnorecaseAndSmartcase": true,
	"vim.hlsearch": true,
	"vim.visualstar": true,
	"vim.startInInsertMode": false,
	// --Cursor | vim
	"vim.cursorStylePerMode.insert": "line",
	"vim.cursorStylePerMode.normal": "block",
	"vim.cursorStylePerMode.visual": "block",
	"vim.cursorStylePerMode.replace": "underline",
	// --status bar | vim
	"vim.statusBarColorControl": true,
	"vim.statusBarColors.visual": "#0E4E4E",
	"vim.statusBarColors.normal": "#1F1B2C",
	"vim.statusBarColors.replace": "#420A7F",
	"vim.statusBarColors.insert": "#500128",
	"vim.statusBarColors.commandlineinprogress": [
		"#5c6300"
	],
	"vim.incsearch": true,
	"vim.easymotionDimBackground": false,
	"vim.targets.smartQuotes.breakThroughLines": false,
	"vim.targets.smartQuotes.aIncludesSurroundingSpaces": false,
	"vim.vimrc.path": "$HOME/.config/vscode/.vscodevimrc",
	"vim.vimrc.enable": true,
	"vim.highlightedyank.enable": true,
	"vim.leader": "<space>",
	"vim.smartRelativeLine": true,
	"vim.foldfix": false,
	"vim.startofline": false,
	"whichkey.bindingOverrides": [
		// {
		//   "keys": "t.s",
		//   "name": "Go to line",
		//   "type": "command",
		//   "command":"workbench.action.gotoLine",
		// },
	],
	// --VisualVim
	"vim.visualModeKeyBindingsNonRecursive": [
		{
			"before": [
				"leader",
				"s"
			],
			"commands": [
				"workbench.action.findInFiles"
			]
		},
		// {
		//   "before": [
		//     "<space>"
		//   ],
		//   "commands": [
		//     "vspacecode.space"
		//   ]
		// },
		{
			"before": [
				">"
			],
			"commands": [
				"editor.action.indentLines"
			]
		},
		{
			"before": [
				"<"
			],
			"commands": [
				"editor.action.outdentLines"
			]
		}
	],
	// --NormalVim
	"vim.normalModeKeyBindings": [
		// {
		//   "before": [
		//     "leader",
		//     "leader"
		//   ],
		//   "commands": [
		//     "whichkey.show"
		//   ]
		// },
		{
			"before": [
				"leader",
				"c"
			],
			"commands": [
				"whichkey.show"
			]
		},
		//Explorer
		{
			"before": [
				"leader",
				"n"
			],
			"commands": [
				"workbench.view.explorer"
			]
		},
		// open new terminal-external
		{
			"before": [
				"leader",
				"leader",
				"leader",
				"'"
			],
			"commands": [
				"workbench.action.files.save"
				// "workbench.action.terminal.openNativeConsole"
			]
		},
		{
			"before": [
				"leader",
				"leader",
				"leader",
				"l"
			],
			"commands": [
				"workbench.action.gotoLine"
			]
		},
		// close others tab 
		{
			"before": [
				"leader",
				"leader",
				"leader",
				"w",
				"q"
			],
			"commands": [
				"workbench.action.closeAllEditors"
			]
		},
		// minimap
		{
			"before": [
				"leader",
				"m"
			],
			"commands": [
				"editor.action.toggleMinimap"
			]
		},
		// move focus to statusbar
		{
			"before": [
				"leader",
				"b"
			],
			"commands": [
				"workbench.action.focusStatusBar"
			]
		},
		// remove all Breakpoints 
		{
			"before": [
				"leader",
				"a",
				"d"
			],
			"commands": [
				"workbench.debug.viewlet.action.removeAllBreakpoints"
			]
		},
		// toggle Breakpoints
		{
			"before": [
				"leader",
				"d"
			],
			"commands": [
				"editor.debug.action.toggleBreakpoint"
			],
		},
		// start debug
		{
			"before": [
				"leader",
				"leader",
				"d"
			],
			"commands": [
				"workbench.action.debug.start",
			]
		},
		// problems
		{
			"before": [
				"leader",
				"p"
			],
			"commands": [
				"workbench.actions.view.problems"
			]
		},
		// {
		//   "before": [
		//     "leader",
		//     "leader",
		//     "leader",
		//     "p"
		//   ],
		//   "after": [
		//     "v"
		//   ],
		//   "commands": [
		//     "editor.action.marker.nextInFiles"
		//   ],
		// },
		// 
		// source control
		{
			"before": [
				"leader",
				"g"
			],
			"commands": [
				"workbench.view.scm"
			],
		},
		// search in project
		{
			"before": [
				"leader",
				"leader",
				"s"
			],
			"commands": [
				"workbench.action.experimental.quickTextSearch"
			]
		},
		// fold 
		{
			"before": [
				"leader",
				"leader",
				"leader",
				"["
			],
			"commands": [
				"editor.fold"
			],
		},
		// unfold
		{
			"before": [
				"leader",
				"leader",
				"leader",
				"]"
			],
			"commands": [
				"editor.unfold"
			],
		},
		// switch windows
		{
			"before": [
				"leader",
				"up"
			],
			"commands": [
				"editor.action.focusStickyScroll"
			]
		},
		// jump to function
		{
			"before": [
				"leader",
				"f"
			],
			"commands": [
				"extension.vim_ctrl+]"
			],
		},
		// recent
		{
			"before": [
				"leader",
				"z"
			],
			"commands": [
				"workbench.action.openRecent"
			]
		},
		// timeline
		{
			"before": [
				"leader",
				"l"
			],
			"commands": [
				"timeline.focus"
			]
		},
		// quit all
		{
			"before": [
				"leader",
				"q"
			],
			"commands": [
				"workbench.action.closeSidebar",
				"workbench.action.closeAuxiliaryBar",
				"workbench.action.closePanel"
			]
		},
		// opened recently tab
		{
			"before": [
				"leader",
				"/"
			],
			"commands": [
				"workbench.action.quickOpenPreviousRecentlyUsedEditorInGroup"
			],
		},
		// terminal
		{
			"before": [
				"leader",
				"t"
			],
			"commands": [
				"workbench.action.terminal.focus"
			],
		},
		// file browse
		{
			"before": [
				"leader",
				"leader",
				"n"
			],
			"commands": [
				"file-browser.open"
			]
		},
		// Go to bracket "{}[]()"
		{
			"before": [
				"'"
			],
			"commands": [
				"editor.action.jumpToBracket"
			]
		},
		{
			"before": [
				"leader",
				"leader",
				"leader",
				"."
			],
			"commands": [
				"editor.action.showDefinitionPreviewHover"
			]
		},
		// select text
		{
			"before": [
				"leader",
				"S"
			],
			"after": [
				"B",
				"v",
				"E",
				// "y"
			],
		},
		{
			"before": [
				"?"
			],
			"commands": [
				"fuzzySearch.activeTextEditor"
			]
		},
		{
			"before": [
				"leader",
				"o"
			],
			"commands": [
				"editor.action.openLink",
			]
		},
		{
			"before": [
				"f"
			],
			"commands": [
				"breadcrumbs.focusAndSelect"
			],
		},
		{
			"before": [
				"\\",
				"h"
			],
			"commands": [
				"workbench.action.moveEditorToLeftGroup"
			],
		},
		{
			"before": [
				"\\",
				"l"
			],
			"commands": [
				"workbench.action.moveEditorToRightGroup"
			],
		},
		{
			"before": [
				"\\",
				"j"
			],
			"commands": [
				"workbench.action.moveEditorToBelowGroup"
			],
		},
		{
			"before": [
				"\\",
				"k"
			],
			"commands": [
				"workbench.action.moveEditorToAboveGroup"
			],
		},
	],
