	//e--------------------------------------------------- 
	"whichkey.bindings": [ //menu-1
		{
			"key": " ",
			"name": "»»»Search keybindings«««",
			"type": "command",
			"command": "whichkey.searchBindings"
		},
		{
			"key": "\t",
			"name": "Last editor",
			"type": "commands",
			"commands": [
				"workbench.action.navigateToLastEditLocation"
				// "workbench.action.quickOpenPreviousRecentlyUsedEditorInGroup",
				// "list.select"
			]
		},
		{
			"key": "F",
			"name": "fzf search",
			"icon": "search",
			"command": "extension.multiCommand.execute",
			"args": {
				"sequence": [
					{
						"command": "command-runner.run",
						"args": {
							"command": "fzf-p | xargs -r code"
						},
					},
					{
						"command": "workbench.action.terminal.moveToEditor",
					},
				]
			}
		},
		{
			"key": "/",
			"name": "Search in project",
			"icon": "search",
			"type": "command",
			"command": "search.action.openNewEditorToSide"
		},
		{
			"key": ".",
			"name": "Repeat most recent action",
			"type": "command",
			"command": "whichkey.repeatMostRecent"
		},
		{
			"key": "b",
			"name": "« Buffers/Editors »",
			"type": "bindings",
			"bindings": [
				{
					"key": "H",
					"name": "Move editor into left group",
					"icon": "triangle-left",
					"type": "command",
					"command": "workbench.action.moveEditorToLeftGroup"
				},
				{
					"key": "L",
					"name": "Move editor into right group",
					"icon": "triangle-right",
					"type": "command",
					"command": "workbench.action.moveEditorToRightGroup"
				},
